{
  "firestore": {
    "rules": "firebase/firestore.rules",
    "indexes": "firebase/firestore.indexes.json"
  },
  "storage": [
    {
      "bucket": "clementine-7568d.firebasestorage.app",
      "rules": "firebase/storage.rules"
    }
  ],
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "src",
        "tsconfig.json",
        ".deploy"
      ]
    }
  ],
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "functions": {
      "port": 5003
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}
